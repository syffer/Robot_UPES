package controller;

import view.ImageView;
import model.BufferedImageModel;

public class ImageController {

	public BufferedImageModel imageModel;
	public ImageView imageView;
	
	public ImageController(BufferedImageModel imageModel, String operationName, double executionTime) {
		this(imageModel, new ImageView(imageModel, operationName, executionTime));
	}
	
	public ImageController(BufferedImageModel imageModel, ImageView imageView) {
		this.imageModel = imageModel;
		this.imageView = imageView;
	}
	
}
