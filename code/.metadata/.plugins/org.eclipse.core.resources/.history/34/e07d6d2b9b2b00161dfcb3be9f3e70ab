package rajan;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

import model.ImageModel;


public class Sequence {
	
	private List<Set<Integer>> sequence;
	
	public Sequence(ImageModel imageModel) {
		this.sequence = new ArrayList<Set<Integer>>();
		
		for(int i = 0; i < imageModel.getWidth(); i++) {
			for(int j = 0; j < imageModel.getHeight(); j++) {
				Set<Integer> set = new HashSet<Integer>();
				set.add(imageModel.get(i,  j));
				this.sequence.add(set);
			}
		}
		
	}
	
	
	public int size() {
		return this.sequence.size();
	}
	
}
